{
    "manifest_version": 3,
    "name": "Form Autofiller",
    "version": "1.1",
    "description": "Fill forms with test data from the popup.",
    "permissions": ["scripting", "activeTab"],
    "action": {
      "default_popup": "popup.html",
      "default_title": "Autofill Form"
    },
    "background": {
      "service_worker": "background.js"
    }
  }
  